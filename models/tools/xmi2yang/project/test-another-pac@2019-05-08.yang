                description "none";
            }
            container test-another-configuration {
                uses test-another-configuration-ref;
                description "none";
            }
            container test-another-status {
                config false;
                uses test-another-status-ref;
                description "none";
            }
            description "none";
        }

    /****************************************
    * package diagrams
    **************************************/ 
        grouping another-lp-spec {
            container another-pac {
                uses test-another-pac;
                description "none";
            }
            description "none";
        }

    grouping test-another-capability-ref {
        leaf capability-1 {
            type string;
            description "none";
        }
        leaf capability-2 {
            type string;
            description "none";
        }
        description "none";
    }
    grouping test-another-configuration-ref {
        leaf configuration-1 {
            type string;
            description "none";
        }
        leaf configuration-2 {
            type string;
            description "none";
        }
        description "none";
    }
    grouping test-another-status-ref {
        leaf status-1 {
            type string;
            config false;
            description "none";
        }
        leaf status-2 {
            type string;
            config false;
            description "none";
        }
        description "none";
    }
