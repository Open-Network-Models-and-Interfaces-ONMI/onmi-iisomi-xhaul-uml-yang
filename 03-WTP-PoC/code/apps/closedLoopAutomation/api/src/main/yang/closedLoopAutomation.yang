module closedLoopAutomation {
    yang-version 1;
    namespace "urn:opendaylight:params:xml:ns:yang:closedLoopAutomation";
    prefix "closedLoopAutomation";

    revision "2016-09-19" {
        description "Initial revision of closedLoopAutomation model";
    }

    rpc start {
        output {
            leaf status {
                type string;
            }
        }
    }

    rpc save-timer {
        input {
           uses timer;
        }

        output {
            leaf status {
                type string;
            }
        }
    }

     rpc read-timer {
        output {
             uses timer;
        }
     }

     container timer-config {
         config true;

         uses timer;
     }

      grouping timer {
        leaf option {
             type enumeration {
                  enum 5seconds;
                  enum 30seconds;
                  enum 1minute;
                  enum 30minutes;
                  enum 1hour;
             }
        }

        leaf enabled {
              type boolean;
        }
      }
}
